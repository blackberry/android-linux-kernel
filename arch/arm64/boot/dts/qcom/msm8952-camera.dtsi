			"vfe_clk_src", "camss_vfe_vfe_clk",
			"camss_csi_vfe_clk", "iface_clk",
			"bus_clk", "iface_ahb_clk";
		qcom,clock-rates = <0 0 266670000 0 0 0 0 0>;
		qos-entries = <8>;
		qos-regs = <0x2c4 0x2c8 0x2cc 0x2d0 0x2d4 0x2d8
			0x2dc 0x2e0>;
