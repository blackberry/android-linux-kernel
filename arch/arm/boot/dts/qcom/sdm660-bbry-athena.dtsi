			red_led: qcom,rgb_0 {
				qcom,ramp-step-ms = <62>;
				qcom,idx-len = <48>;
				qcom,duty-pcts =
					[00 01 02 03 05 07 0a 0f 14 19 1e 23 28 2d 32 37 3c 41 46 4b 50 57 5e 64
                                         64 5e 57 50 4b 46 41 3c 37 32 2d 28 23 1e 19 14 0f 0a 06 03 02 01 00 00];
			};
			green_led: qcom,rgb_1 {
				qcom,ramp-step-ms = <62>;
				qcom,start-idx = <0>;
				qcom,idx-len = <48>;
				qcom,duty-pcts =
					[00 01 02 03 05 07 0a 0f 14 19 1e 23 28 2d 32 37 3c 41 46 4b 50 57 5e 64
                                         64 5e 57 50 4b 46 41 3c 37 32 2d 28 23 1e 19 14 0f 0a 06 03 02 01 00 00];
				qcom,use-blink;
			};

			blue_led: qcom,rgb_2 {
				qcom,ramp-step-ms = <62>;
				qcom,start-idx = <0>;
				qcom,idx-len = <48>;
				qcom,duty-pcts =
					[00 01 02 03 05 07 0a 0f 14 19 1e 23 28 2d 32 37 3c 41 46 4b 50 57 5e 64
                                         64 5e 57 50 4b 46 41 3c 37 32 2d 28 23 1e 19 14 0f 0a 06 03 02 01 00 00];
				qcom,use-blink;
			};
		};
